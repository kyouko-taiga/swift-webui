server {
    listen 80;
    server_name server_domain_or_IP;

    location / {
        try_files   $uri @app;
    }

    location @app {
        include     uwsgi_params;
        uwsgi_pass  unix:///tmp/uwsgi.sock;
        uwsgi_param UMIUSHI_SETTINGS .settings.py;
    }

    location /static {
        alias       /static;
    }
}
